<div class="b-block-career-blog js-show-more" data-visable-count="9">
    <div class="b-block-career-blog__wrap js-resizable-wrap">
        {{#each list}}
        <a href="{{href}}" class="b-block-career-blog__item js-card">
            <div class="b-block-career-blog__item__img">
                {{{getImg src=img}}}
            </div>
            <div class="b-block-career-blog__item__label">{{label}}</div>
        </a href="{{href}}">
        {{/each}}
    </div>
    <div class="b-block-career-blog__btn btn-blue js-trigger">Загрузить ещё</div>
</div>