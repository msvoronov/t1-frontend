<div class="block-openschool-all">
  <div class="block-openschool-all__wrap">
    {{#each list}}
      <a href="{{href}}" class="block-openschool-all__item">
        <div class="block-openschool-all__item__img">{{{getImg src=img}}}</div>
        <div class="block-openschool-all__item__label">{{label}}</div>
        {{#if descr}}
          <div class="block-openschool-all__item__descr">{{descr}}</div>
        {{/if}}
      </a>
    {{/each}}
  </div>
</div>